{
  "pipeline": {
    "name": "template",
    "display_name": "Template Pipeline",
    "data_file": "template.json",
    "anki_note_type": "Template Note Type",
    "stages": [
      "analyze",
      "prepare",
      "generate", 
      "validate",
      "sync"
    ]
  },
  "stage_providers": {
    "generate": "default_media",
    "sync": "anki"
  },
  "processing_settings": {
    "batch_size": 10,
    "max_concurrent": 3,
    "timeout_seconds": 300
  },
  "validation": {
    "strict_mode": true,
    "require_all_fields": true,
    "check_duplicates": true
  },
  "custom_settings": {
    "example_setting": "example_value",
    "feature_flags": {
      "enable_advanced_processing": false,
      "use_experimental_validation": false
    }
  }
}